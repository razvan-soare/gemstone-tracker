{
	"expo": {
		"name": "Gemstone tracker",
		"slug": "gem-tracker",
		"scheme": "gemstone-tracker",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "automatic",
		"assetBundlePatterns": ["**/*"],
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true,
			"config": {
				"usesNonExemptEncryption": false
			},
			"web": {
				"bundler": "metro",
				"output": "single",
				"favicon": "./assets/favicon.png"
			},
			"splash": {
				"image": "./assets/splash.png",
				"resizeMode": "cover",
				"backgroundColor": "#ffffff",
				"dark": {
					"backgroundColor": "#000000",
					"resizeMode": "cover",
					"image": "./assets/splash.png"
				}
			},
			"bundleIdentifier": "com.razvansoare.gemtracker"
		},
		"android": {
			"adaptiveIcon": {
				"backgroundImage": "./assets/adaptive-icon/background.png",
				"foregroundImage": "./assets/adaptive-icon/foreground.png"
			},
			"splash": {
				"image": "./assets/splash.png",
				"resizeMode": "cover",
				"backgroundColor": "#ffffff",
				"dark": {
					"backgroundColor": "#000000",
					"resizeMode": "cover",
					"image": "./assets/splash.png"
				}
			}
		},
		"experiments": {
			"typedRoutes": true
		},
		"plugins": ["expo-router", "expo-secure-store"],
		"extra": {
			"eas": {
				"projectId": "37d4e43f-67a6-4de6-90a3-81495f8c7666"
			}
		},
		"owner": "razvansoare"
	}
}
